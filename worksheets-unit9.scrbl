#lang curr/lib

@declare-tags[group pedagogy selftaught bootstrap]

@worksheet{
   Catchy Intro:    
   
   @free-response[#:id "intro"]
   
   Name, Age, Grade:
   @fill-in-the-blank[#:id "info"
                                #:label "Info"
                                #:columns 120]
   
   Game Title:
   @fill-in-the-blank[#:id "title"
                                #:label "Title"
                                #:columns 90] 
                                             
   Back Story:    
   
   @free-response[#:id "story"]
   
   Characters:    
   
   @free-response[#:id "characters"]
   
   Explain a piece of your code:    
   
   @free-response[#:id "explain-code"]
           
           }